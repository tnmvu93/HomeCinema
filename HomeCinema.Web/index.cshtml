@{
    Layout = null;
}

<!DOCTYPE html>
<html ng-app="homeCinema">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home Cinema</title>
	<meta charset="utf-8" />
    <link href="content/css/bootstrap.css" rel="stylesheet" />
    <link href="content/css/bootstrap-theme.css" rel="stylesheet" />
</head>
<body ng-controller="rootController">
    <top-bar></top-bar>
    <div class="row-offcanvas row-offcanvas-left">
        <side-bar></side-bar>
        <div id="main">
            <div class="col-md-12">
                <p class="visible-xs">
                    <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas">
                        <i class="glyphicon glyphicon-chevron-left"></i>
                    </button>
                </p>
                <div class="page {{ pageClass }}" ng-view></div>
            </div>
        </div>
    </div>
    <script src="scripts/vendor/jquery.min.js"></script>
    <script src="scripts/vendor/bootstrap.min.js"></script>

    <script src="scripts/vendor/angular.js"></script>
    <script src="scripts/vendor/angular-resource.min.js"></script>
    @*<script src="scripts/vendor/angular.min.js"></script>*@
    <script src="scripts/vendor/angular-route.min.js"></script>
    <script src="scripts/vendor/angular-base64.min.js"></script>
    <script src="scripts/vendor/angular-cookies.min.js"></script>
    <script src="scripts/vendor/angular-validator.min.js"></script>
    <script src="scripts/vendor/angular-file-upload.min.js"></script>
    <script src="scripts/vendor/angucomplete-alt.min.js"></script>
    <script src="scripts/vendor/ui-bootstrap.min.js"></script>
    <script src="scripts/vendor/ui-bootstrap-tpls.min.js"></script>

    <script src="scripts/vendor/loading-bar.min.js"></script>

    <script src="scripts/spa/modules/common.core.js"></script>
    <script src="scripts/spa/modules/common.ui.js"></script>
    <script src="scripts/spa/app.js"></script>
    <script src="scripts/spa/layout/sideBar.directive.js"></script>
</body>
</html>
